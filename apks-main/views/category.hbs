	<!--Contant Area-->
	<section>
		<div class="container">


			<div class="h1 mt-4">
				All Categories
			</div>
			<a href="addcategory" class="btn btn-warning float-right my-2">Add Category</a>
			<div class="clearfix"></div>
			<table class="table table-bordered table-hover table-condensed table-export" cellspacing="0" width="100%">
				<thead class="bg-info">
					<tr>
						<th>
							#
						</th>
						<th>
							Category Name
						</th>
						<th>
							SubCategories
						</th>
						<th>
							Actions
						</th>
					</tr>
				</thead>
				<tbody>
					{{#each cate}}
					<tr>
						<td>
							{{inc @index}}
						</td>
						<td>
							{{this.category}}
						</td>
						<td>
							<div>{{this.slug}}</div>
						</td>
						<td class="text-right">
							{{#if this.static}}
							{{else}}
							<a id="deleteItem"   onclick="Deletecate({{{json this.category}}})"  class="btn m-2 btn-circle btn-danger">Delete</a>
								{{/if}}
								<a href="/subcategory?cate={{this.category}}" class="btn btn-circle btn-primary">
									SubCategories</a>
						</td>
					</tr>
					{{/each}}
				</tbody>
			</table>

		</div>
